{
  "compilerOptions": {
    "target": "es5",                                
    "module": "commonjs",                          
    "strict": true,                                
    "esModuleInterop": true,                     
    "skipLibCheck": true,                          
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": { 
      "@src/*": ["src/*"] , 
      "@common/*": ["src/common/*"] , 
      "@types/*": ["src/types/*"]
    }
  }
}
